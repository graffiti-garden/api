{
  "name": "@graffiti-garden/api",
  "version": "0.1.9",
  "description": "The heart of Graffiti",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "node": "./dist/index.cjs.js",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./tests": {
      "types": "./dist/tests/src/index.d.ts",
      "default": "./dist/tests/index.js"
    }
  },
  "files": [
    "src",
    "dist",
    "tests",
    "package.json",
    "README.md"
  ],
  "author": "Theia Henderson",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "docs": "typedoc --options typedoc.json",
    "build-api": "rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "build-tests": "cd tests && rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
    "build": "npm run build-api && npm run build-tests",
    "prepublishOnly": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graffiti-garden/api.git"
  },
  "bugs": {
    "url": "https://github.com/graffiti-garden/api/issues"
  },
  "homepage": "https://api.graffiti.garden/classes/Graffiti.html",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.31.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.26.11",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15",
    "ajv": "^8.17.1",
    "fast-json-patch": "^3.1.1"
  }
}
